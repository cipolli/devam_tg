div.page-header
  div.name Projetos
  div.options
    div.option
      a.link(data-ng-href='{{systemUri.projeto()}}') Listagem
    div.option
      a.link(data-ng-href='{{systemUri.projetoCadastro()}}') Cadastro

div.projeto-container
  div.lista
    div.item(data-ng-repeat='contaProjeto in projetosUsuario track by $index')
      div.linha.comColunas
        div.indices
          span.textodestaque(data-ng-bind="contaProjeto.projeto.nome")
        div.indices.itemDireita
          div.irModulos.cursorPointer(data-ng-click='irParaModulos(contaProjeto.projeto)')
            i.icon(class='material-icons') view_module
            span.link Ir para m√≥dulos
      div.linha
        div.indices
          span.textosecundario Modulos {{projeto.modulos.length}}
        div.indices.projeto.repositorioGitpe
            span.textosecundario.label Repositorio {{projeto.repositorioGit}}

